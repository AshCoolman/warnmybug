<!DOCTYPE html>
<html>
<head>
	<title>Warnmybug</title>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'wmb/wmb', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
</head>
<body>
	<div class="blog-masthead">
		<div class="container">
			<nav class="blog-nav">
				<%= link_to t('menu.projects'), projects_path, class: "blog-nav-item active"%>
				<%= link_to(t('menu.sign_out'), destroy_user_session_path, :method => :delete, class: "blog-nav-item pull-right") %>
			</nav>
		</div>
	</div>
	<div class="container">
	<%= yield %>
	</div>

</body>
</html>
