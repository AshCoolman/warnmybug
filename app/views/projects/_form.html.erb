<%= form_for(@project) do |f| %>
	<%= render 'shared/error', object: @project %>
	
	<div class="form-group">
	  <%= f.label :name %>
	  <%= f.text_field :name, class: "form-control", placeholder: :name %>
	</div>
  <div class="modal-footer">
    <%= f.submit class: "btn btn-success" %>
  </div>
<% end %>
