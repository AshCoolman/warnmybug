<h3>
	<%=t('title.project.list')%>
	<%= link_to t('form.new'), new_project_path, class: "pull-right btn btn-default" %>
</h3>
<hr />
<table class="table table-hover">
  <thead>
    <tr>
      <th><%=t('attributes.name')%></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= link_to project.name, project %></td>
      </tr>
    <% end %>
  </tbody>
</table>
